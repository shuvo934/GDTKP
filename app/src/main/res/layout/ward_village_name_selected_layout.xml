<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/extra_text_w_v_p_crtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=">>"
        android:textSize="13sp"
        android:layout_alignParentStart="true"
        android:fontFamily="@font/cantora_one"
        android:layout_centerVertical="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/ward_village_name_p_crtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/extra_text_w_v_p_crtn"
        android:fontFamily="@font/cantora_one"
        android:text="889339.gpx"
        android:layout_toStartOf="@id/delete_ward_villa_name_p_crtn"
        android:textSize="13sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/delete_ward_villa_name_p_crtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:layout_centerVertical="true"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="15dp"
        android:src="@drawable/cancel_24"
        android:layout_gravity="end"
        />

</RelativeLayout>